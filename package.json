{
  "name": "n8n-nodes-variables-store",
  "version": "1.0.0",
  "description": "n8n community node for global variables management - alternative to paid Variables feature",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "variables",
    "global",
    "storage"
  ],
  "license": "MIT",
  "homepage": "https://github.com/betty/n8n-nodes-variables-store",
  "author": {
    "name": "Betty",
    "email": "betty@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betty/n8n-nodes-variables-store.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint . --ext .js,.ts",
    "lintfix": "eslint . --ext .js,.ts --fix",
    "test": "node test/VariablesStorage.test.js",
    "prepublishOnly": "npm run build && npm run test"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/VariablesGet/VariablesGet.node.js",
      "dist/nodes/VariablesSet/VariablesSet.node.js",
      "dist/nodes/VariablesManager/VariablesManager.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.19.112",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
